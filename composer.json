{
	"name": "craigrodway/classroombookings",
	"description": "Classroombookings",
	"type": "project",
	"keywords": ["roombooking", "booking", "scheduling", "education", "schools"],
	"homepage": "https://classroombookings.com/",
	"require": {
		"php": ">=5.5.0",
		"codeigniter/framework": "^3",
		"pixelrobin/php-feather": "dev-feather-dependency",
		"jupitern/table": "^2"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/craigrodway/php-feather.git"
		},
		{
	        "type": "composer",
	        "url": "https://asset-packagist.org"
	    }
	],
	"license": "AGPLv3",
	"authors": [
		{
			"name": "Craig A Rodway",
			"email": "craig.rodway@gmail.com"
		}
	],
	"config": {
		"vendor-dir": "application/vendor"
	},
	"autoload": {
		"psr-4": {
			"app\\": "application/"
		}
	},
	"archive": {
		"exclude": [
			"!application/vendor/*",
			"application/vendor/codeigniter/framework/application",
			"application/vendor/codeigniter/framework/tests",
			"application/vendor/codeigniter/framework/user_guide",
			".gitignore",
			".editorconfig",
			"composer.json",
			"composer.lock"
		]
	},
	"scripts": {
		"create-migration": "@php index.php commands/create_migration"
	}
}
